{
  "name": "otsukailist-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "deploy": "npm run build && npx wrangler deploy",
    "deploy:preview": "npm run build && npx wrangler deploy --dry-run",
    "lint": "eslint --ext .vue,.ts src/",
    "lint:fix": "eslint --ext .vue,.ts src/ --fix",
    "format": "prettier -w src/*.{ts,vue} && prettier -w src/**/*.{ts,vue}",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run"
  },
  "dependencies": {
    "tailwind-merge": "^3.4.0",
    "vue": "^3.5.21",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@tailwindcss/vite": "^4.1.13",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/ui": "^4.0.16",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.36.0",
    "eslint-plugin-vue": "^10.5.0",
    "globals": "^16.4.0",
    "jiti": "^2.6.0",
    "jsdom": "^27.4.0",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.13",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.44.1",
    "vite": "^7.1.7",
    "vitest": "^4.0.16",
    "vue-tsc": "^3.0.7"
  }
}
